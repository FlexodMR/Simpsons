// Hairclub Tune Lighter
// Creates toon like lighting discontunities
ps.1.1    // pc pixel shader

tex t0              //base map
tex t1              //Toon lighting map
mul r0, t1, t0
mul r0, r0, c1
mul r1, t0, v0
mad r0, c1, r1, r0
;    + mov r0.a, t0.a             ;;TODO turn this on when you want to have alpha on characters


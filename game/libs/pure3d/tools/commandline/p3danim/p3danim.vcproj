<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="p3danim"
	SccProjectName="p3danim"
	SccAuxPath=""
	SccLocalPath=".."
	SccProvider="MSSCCI:Perforce SCM">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Release|Win32"
			OutputDirectory=".\Release"
			IntermediateDirectory=".\Release"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="2">
			<Tool
				Name="VCCLCompilerTool"
				InlineFunctionExpansion="1"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="../../../..,../../../toollib/inc,../../../constants,../../../toollib/chunks16/inc,../../../../radmath"
				PreprocessorDefinitions="NDEBUG,_CONSOLE,WIN32,P3DWIN95,RAD_PC,RAD_WIN32,RAD_RELEASE"
				StringPooling="TRUE"
				RuntimeLibrary="0"
				EnableFunctionLevelLinking="TRUE"
				RuntimeTypeInfo="TRUE"
				PrecompiledHeaderFile=".\Release/p3danim.pch"
				AssemblerListingLocation=".\Release/"
				ObjectFile=".\Release/"
				ProgramDataBaseFileName=".\Release/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="3"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="odbc32.lib odbccp32.lib"
				OutputFile="../bin/p3danim.exe"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				IgnoreDefaultLibraryNames="libc.lib, msvcrt.lib, libcd.lib, libcmtd.lib, msvcrtd.lib "
				ProgramDatabaseFile=".\Release/p3danim.pdb"
				SubSystem="1"/>
			<Tool
				Name="VCMIDLTool"
				TypeLibraryName=".\Release/p3danim.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory=".\Debug"
			IntermediateDirectory=".\Debug"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="2">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="../../../..,../../../toollib/inc,../../../constants,../../../toollib/chunks16/inc,../../../../radmath"
				PreprocessorDefinitions="_CONSOLE,P3DDEBUG,P3DWIN95,WIN32IO_IS_STDIO,_DEBUG,WIN32,RAD_WIN32,RAD_PC,RAD_DEBUG"
				RuntimeLibrary="1"
				RuntimeTypeInfo="TRUE"
				PrecompiledHeaderFile=".\Debug/p3danim.pch"
				AssemblerListingLocation=".\Debug/"
				ObjectFile=".\Debug/"
				ProgramDataBaseFileName=".\Debug/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="3"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="odbc32.lib odbccp32.lib"
				OutputFile=".\Debug/p3danim.exe"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				IgnoreDefaultLibraryNames="libc.lib, libcmt.lib, msvcrt.lib, libcd.lib, msvcrtd.lib"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile=".\Debug/p3danim.pdb"
				SubSystem="1"/>
			<Tool
				Name="VCMIDLTool"
				TypeLibraryName=".\Debug/p3danim.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Source"
			Filter="*.cpp,*.c">
			<File
				RelativePath=".\Param.cpp">
			</File>
			<File
				RelativePath=".\main.cpp">
			</File>
			<File
				RelativePath=".\version.cpp">
			</File>
		</Filter>
		<Filter
			Name="Include"
			Filter="*.h,*.hpp">
			<File
				RelativePath=".\Param.hpp">
			</File>
			<File
				RelativePath=".\version.hpp">
			</File>
		</Filter>
		<File
			RelativePath=".\p3danim.sc">
			<FileConfiguration
				Name="Release|Win32">
				<Tool
					Name="VCCustomBuildTool"
					Description="ToolsParamCreator"
					CommandLine="..\bin\ToolsParamCreator  -s  &quot;$(InputName).sc&quot;"
					AdditionalDependencies="../ToolsParamCreator/Param_hpp_cpp.tpl;"
					Outputs="Param.hpp;Param.cpp"/>
			</FileConfiguration>
			<FileConfiguration
				Name="Debug|Win32">
				<Tool
					Name="VCCustomBuildTool"
					Description="ToolsParamCreator"
					CommandLine="..\bin\ToolsParamCreator  -s  &quot;$(InputName).sc&quot;"
					AdditionalDependencies="../ToolsParamCreator/Param_hpp_cpp.tpl;"
					Outputs="Param.hpp;Param.cpp"/>
			</FileConfiguration>
		</File>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>

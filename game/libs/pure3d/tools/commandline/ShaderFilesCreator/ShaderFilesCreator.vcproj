			</File>
			<File
				RelativePath="..\coder\schemaparse\construct.h">
			</File>
			<File
				RelativePath="..\coder\schemaparse\schema.tab.c">
			</File>
			<File
				RelativePath="..\coder\schemaparse\schemaparse.hpp">
			</File>
			<Filter
				Name="meta"
				Filter="l;y">
				<File
					RelativePath="..\coder\schemaparse\schema.l">
					<FileConfiguration
						Name="Debug|Win32">
						<Tool
							Name="VCCustomBuildTool"
							CommandLine="..\coder\schemaparse\flex++bison++\flex++.exe -o..\coder\schemaparse\lex.yy.c  ..\coder\schemaparse\schema.l"
							Outputs="..\coder\schemaparse\lex.yy.c"/>
					</FileConfiguration>
					<FileConfiguration
						Name="Release|Win32">
						<Tool
							Name="VCCustomBuildTool"
							CommandLine="..\coder\schemaparse\flex++bison++\flex++.exe -o..\coder\schemaparse\lex.yy.c  ..\coder\schemaparse\schema.l"
							Outputs="..\coder\schemaparse\lex.yy.c"/>
					</FileConfiguration>
				</File>
				<File
					RelativePath="..\coder\schemaparse\schema.y">
					<FileConfiguration
						Name="Debug|Win32">
						<Tool
							Name="VCCustomBuildTool"
							CommandLine="..\coder\schemaparse\flex++bison++\bison++.exe -o  ..\coder\schemaparse\schema.tab.c ..\coder\schemaparse\schema.y"
							Outputs="..\coder\schemaparse\schema.tab.c"/>
					</FileConfiguration>
					<FileConfiguration
						Name="Release|Win32">
						<Tool
							Name="VCCustomBuildTool"
							CommandLine="..\coder\schemaparse\flex++bison++\bison++.exe -o  ..\coder\schemaparse\schema.tab.c ..\coder\schemaparse\schema.y"
							Outputs="..\coder\schemaparse\schema.tab.c"/>
					</FileConfiguration>
				</File>
			</Filter>
		</Filter>
		<Filter
			Name="Resource Files"
			Filter="ico;cur;bmp;dlg;rc2;rct;bin;rgs;gif;jpg;jpeg;jpe">
		</Filter>
		<File
			RelativePath=".\ReadMe.txt">
		</File>
		<File
			RelativePath=".\header_cpp.tpl">
		</File>
		<File
			RelativePath=".\shader_hpp_cpp.tpl">
			<FileConfiguration
				Name="Debug|Win32">
				<Tool
					Name="VCCustomBuildTool"
					CommandLine="..\bin\coder.exe  -t &quot;$(InputName).tpl&quot; -o main.cpp"
					Outputs="main.cpp"/>
			</FileConfiguration>
			<FileConfiguration
				Name="Release|Win32">
				<Tool
					Name="VCCustomBuildTool"
					CommandLine="..\bin\coder.exe  -t &quot;$(InputName).tpl&quot; -o main.cpp"
					Outputs="main.cpp"/>
			</FileConfiguration>
		</File>

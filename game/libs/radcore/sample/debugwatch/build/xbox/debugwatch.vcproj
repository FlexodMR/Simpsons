<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="debugwatch"
	SccProjectName="debugwatch"
	SccAuxPath=""
	SccLocalPath="..\.."
	SccProvider="MSSCCI:Perforce SCM">
	<Platforms>
		<Platform
			Name="Xbox"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Tune|Xbox"
			OutputDirectory=".\xtune"
			IntermediateDirectory=".\xtune"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				InlineFunctionExpansion="1"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="..\..\..\..\inc"
				PreprocessorDefinitions="NDEBUG,WIN32,_XBOX,RAD_XBOX,RAD_TUNE"
				StringPooling="TRUE"
				RuntimeLibrary="4"
				EnableFunctionLevelLinking="TRUE"
				RuntimeTypeInfo="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderFile=".\xtune/debugwatch.pch"
				AssemblerListingLocation=".\xtune/"
				ObjectFile=".\xtune/"
				ProgramDataBaseFileName=".\xtune/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386 /FIXED:NO"
				AdditionalDependencies="xapilib.lib d3d8.lib d3dx8.lib xgraphics.lib dsound.lib dmusic.lib xnet.lib xboxkrnl.lib"
				OutputFile="..\..\bin\debugwatchxt.exe"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="..\..\..\lib"
				ProgramDatabaseFile=".\xtune/debugwatchxt.pdb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="XboxDeploymentTool"/>
			<Tool
				Name="XboxImageTool"
				StackSize="0x10000"
				SuppressStartupBanner="TRUE"/>
		</Configuration>
		<Configuration
			Name="Debug|Xbox"
			OutputDirectory=".\xdebug"
			IntermediateDirectory=".\xdebug"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="..\..\..\..\inc"
				PreprocessorDefinitions="_DEBUG,WIN32,_XBOX,RAD_XBOX,RAD_DEBUG"
				RuntimeLibrary="5"
				RuntimeTypeInfo="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderFile=".\xdebug/debugwatch.pch"
				AssemblerListingLocation=".\xdebug/"
				ObjectFile=".\xdebug/"
				ProgramDataBaseFileName=".\xdebug/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="3"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386 /FIXED:NO"
				AdditionalDependencies="xapilibd.lib d3d8d.lib d3dx8d.lib xgraphicsd.lib dsoundd.lib dmusicd.lib xnetd.lib xboxkrnl.lib"
				OutputFile="..\..\bin\debugwatchxd.exe"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="..\..\..\lib"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile=".\xdebug/debugwatchxd.pdb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="XboxDeploymentTool"/>
			<Tool
				Name="XboxImageTool"
				StackSize="0x10000"
				IncludeDebugInfo="TRUE"
				SuppressStartupBanner="TRUE"/>
		</Configuration>
		<Configuration
			Name="Release|Xbox"
			OutputDirectory=".\xrelease"
			IntermediateDirectory=".\xrelease"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				InlineFunctionExpansion="1"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="..\..\..\..\inc"
				PreprocessorDefinitions="NDEBUG,WIN32,_XBOX,RAD_XBOX,RAD_RELEASE"
				StringPooling="TRUE"
				RuntimeLibrary="4"
				EnableFunctionLevelLinking="TRUE"
				RuntimeTypeInfo="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderFile=".\xrelease/debugwatch.pch"
				AssemblerListingLocation=".\xrelease/"
				ObjectFile=".\xrelease/"
				ProgramDataBaseFileName=".\xrelease/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386 /FIXED:NO"
				AdditionalDependencies="xapilib.lib d3d8.lib d3dx8.lib xgraphics.lib dsound.lib dmusic.lib xnet.lib xboxkrnl.lib"
				OutputFile="..\..\bin\debugwatchxr.exe"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="..\..\..\lib"
				ProgramDatabaseFile=".\xrelease/debugwatchxr.pdb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="XboxDeploymentTool"/>
			<Tool
				Name="XboxImageTool"
				StackSize="0x10000"
				SuppressStartupBanner="TRUE"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;r;odl;idl;hpj;bat">
			<File
				RelativePath="..\..\src\main.cpp">
			</File>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h;hpp;hxx;hm;inl">
		</Filter>
		<Filter
			Name="Resource Files"
			Filter="ico;cur;bmp;dlg;rc2;rct;bin;rgs;gif;jpg;jpeg;jpe">
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>

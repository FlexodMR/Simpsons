<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="simplemovie"
	SccProjectName="simplemovie"
	SccAuxPath=""
	SccLocalPath="."
	SccProvider="MSSCCI:Perforce SCM">
	<Platforms>
		<Platform
			Name="Xbox"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Release|Xbox"
			OutputDirectory=".\simplemovie___Xbox_Release"
			IntermediateDirectory=".\simplemovie___Xbox_Release"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				InlineFunctionExpansion="1"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="..\..\..\..\radmovie\inc,..\..\..\..\radcontent\inc,..\..\..\..\radmath,..\..\..\..\pure3d,..\..\..\..\radcore\inc,..\..\..\..\radsound\inc"
				PreprocessorDefinitions="NDEBUG,RAD_RELEASE,WIN32,_XBOX,RAD_XBOX,RAD_CONSOLE"
				StringPooling="TRUE"
				RuntimeLibrary="4"
				EnableFunctionLevelLinking="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderFile=".\simplemovie___Xbox_Release/simplemovie.pch"
				AssemblerListingLocation=".\simplemovie___Xbox_Release/"
				ObjectFile=".\simplemovie___Xbox_Release/"
				ProgramDataBaseFileName=".\simplemovie___Xbox_Release/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386 /FIXED:NO"
				AdditionalDependencies="xapilib.lib d3d8.lib xgraphics.lib dsound.lib dmusic.lib xnet.lib xmv.lib xboxkrnl.lib binkxbox.lib"
				OutputFile=".\simplemovie___Xbox_Release/simplemovie.exe"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="../../../../sdks/xbox/bink"
				ProgramDatabaseFile=".\simplemovie___Xbox_Release/simplemovie.pdb"
				OptimizeReferences="2"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="XboxDeploymentTool"/>
			<Tool
				Name="XboxImageTool"
				StackSize="0x10000"
				SuppressStartupBanner="TRUE"/>
		</Configuration>
		<Configuration
			Name="Debug|Xbox"
			OutputDirectory=".\simplemovie___Xbox_Debug"
			IntermediateDirectory=".\simplemovie___Xbox_Debug"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="..\..\..\..\radmovie\inc,..\..\..\..\radcontent\inc,..\..\..\..\radmath,..\..\..\..\pure3d,..\..\..\..\radcore\inc,..\..\..\..\radsound\inc"
				PreprocessorDefinitions="_DEBUG,RAD_DEBUG,WIN32,_XBOX,RAD_XBOX,RAD_CONSOLE"
				RuntimeLibrary="5"
				UsePrecompiledHeader="2"
				PrecompiledHeaderFile=".\simplemovie___Xbox_Debug/simplemovie.pch"
				AssemblerListingLocation=".\simplemovie___Xbox_Debug/"
				ObjectFile=".\simplemovie___Xbox_Debug/"
				ProgramDataBaseFileName=".\simplemovie___Xbox_Debug/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="3"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386 /FIXED:NO"
				AdditionalDependencies="xapilibd.lib d3d8d.lib xgraphicsd.lib dsoundd.lib dmusicd.lib xnetd.lib xmvd.lib xboxkrnl.lib binkxbox.lib"
				OutputFile=".\simplemovie___Xbox_Debug/simplemovie.exe"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="../../../../sdks/xbox/bink"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile=".\simplemovie___Xbox_Debug/simplemovie.pdb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="XboxDeploymentTool"/>
			<Tool
				Name="XboxImageTool"
				StackSize="0x10000"
				IncludeDebugInfo="TRUE"
				SuppressStartupBanner="TRUE"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;r;odl;idl;hpj;bat">
			<File
				RelativePath="..\..\src\main.cpp">
				<FileConfiguration
					Name="Release|Xbox">
					<Tool
						Name="VCCLCompilerTool"
						AdditionalIncludeDirectories="..\..\..\inc,..\..\..\..\radcore\inc,..\..\..\..\radsound\inc,..\..\..\..\pure3d,..\..\..\..\radmath;..\..\..\..\radcore\inc ..\..\..\..\..\radsound\inc ..\..\..\..\..\pure3d\ ..\..\..\..\..\radmath\ ..\..\..\..\inc;$(NoInherit)"/>
				</FileConfiguration>
				<FileConfiguration
					Name="Debug|Xbox">
					<Tool
						Name="VCCLCompilerTool"
						AdditionalIncludeDirectories="..\..\..\inc,..\..\..\..\radcore\inc,..\..\..\..\radsound\inc,..\..\..\..\pure3d,..\..\..\..\radmath,..\..\..\..\radcontent\inc;..\..\..\..\pure3d\;..\..\..\..\radmath\;$(NoInherit)"/>
				</FileConfiguration>
			</File>
			<File
				RelativePath="..\..\src\util.cpp">
			</File>
			<File
				RelativePath="..\..\src\util.hpp">
			</File>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h;hpp;hxx;hm;inl">
		</Filter>
		<Filter
			Name="Resource Files"
			Filter="ico;cur;bmp;dlg;rc2;rct;bin;rgs;gif;jpg;jpeg;jpe">
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
